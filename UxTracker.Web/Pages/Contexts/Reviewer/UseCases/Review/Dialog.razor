<MudDialog MaxWidth="MaxWidth.Medium" FullWidth="false">
  <TitleContent>
    Termo de Aceite
  </TitleContent>
  <DialogContent>
    <iframe src="Termo/Profile.pdf" style="width: 100%; height: 700px;" frameborder="0"></iframe>
  </DialogContent>
  <DialogActions>
    <MudButton Color="Color.Primary" OnClick="Submit">Aceitar</MudButton>
  </DialogActions>
</MudDialog>

@code {
  [CascadingParameter]
  private MudDialogInstance MudDialog { get; set; }

  private void Submit() => MudDialog.Close(DialogResult.Ok(true));

}
