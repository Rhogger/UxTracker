@using UxTracker.Web.Components

<CascadingAuthenticationState>
    <Router AppAssembly="@typeof(App).Assembly">
        <Found Context="routeData">
            <AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(HeadlessLayout)">
                <Authorizing>
                    <Authorizing/>
                </Authorizing>
                
                <NotAuthorized>
                    <RedirectToUnauthorizedPage/>
                </NotAuthorized>
            </AuthorizeRouteView>
        </Found>
        <NotFound>
            <RedirectToNotFoundPage />
        </NotFound>
    </Router>
</CascadingAuthenticationState>